\setbeamercolor{frametitle}{bg=a2, fg=black}
\setbeamercolor{title}{bg=b, fg=black}
\setbeamercolor{subtitle}{bg=b, fg=a1}
\setbeamercolor{date}{bg=b, fg=a3}
\setbeamercolor{website}{bg=b, fg=a1}


\setbeamercolor{structure}{fg=a3}

\setbeamercolor{background canvas}{parent=normal text}
\setbeamercolor{background}{parent=background canvas}

